extends layout

block style

block script

block content
  #mainBox.container-fluid
    .content
      #all-table.row
        h3 모든 조사
        table.table.table-hover
          thead.text-center
            tr
              td.col-md-6.col-xs-6 제목
              td.col-md-2.col-xs-2 조회수
              td.col-md-2.col-xs-2 등록일
              td.col-md-2.col-xs-2 마감일
          tbody
            each vote in vote_list
              tr
                td
                  a(href='/vote/'+vote.id)= vote.question
                td.text-center= vote.count
                td.text-center= moment(vote.create_time).format('YYYY-MM-DD HH:mm')
                td.text-center= moment(vote.finish_time).format('YYYY-MM-DD HH:mm')
  #footer.container.visible-md.visible-lg
    hr
    .row
      .col-md-4
        strong 2015 홀가분 (HOLGABUN)
      .col-md-3.col-md-offset-5.text-right
        a(href="mailto:enon.jeong@gmail.com") enon.jeong@gmail.com